{
  "extends": ["tslint:latest", "tslint-react"],
  "rules": {
    "jsx-wrap-multiline": false,
    "jsx-no-multiline-js": false,
    "jsx-no-lambda": false,
    "no-empty": false,
    "trailing-comma": false,
    "no-submodule-imports": false,
    "quotemark": false,
    "interface-name": false
  },
  "linterOptions": {
    "exclude": ["./src/serviceWorker.ts"]
  }
}
